<body >

<script>
function textfocus(element)
{
	ticker = document.getElementById(element + '_ticker');
	if(ticker.value == 0)
	{
		input = document.getElementById(element);
		input.value="";
		input.className="inputMode " + document.getElementById(element + '_class').value;
	}
	ticker.value = 1;
}

function textblur(element)
{
	ticker = document.getElementById(element + '_ticker');
	input = document.getElementById(element);
	if (input.value=="") {
		if(element == 'url')
			init_url();
		else
			init_search();
	}
}

function cleardefaults()
{
	textfocus('url');
	textfocus('search');
}

function init_url()
{
	document.getElementById('url_class').value = document.create.url.className;
	if(document.create.url.value=='')
	{
		document.create.url.className="exampleMode " + document.getElementById('url_class').value;
		document.getElementById('url_ticker').value=0;
		document.create.url.value="http://example.com/rss.xml";
	} else {
		document.getElementById('url_ticker').value = 1;
	}
	return true;
}

function init_search()
{
	document.getElementById('search_class').value = document.create.search.className;
	search = document.create.search;
	if(search.value=='')
	{
//		search.className="hiddenMode smalltext " + document.getElementById('search_class').value;
		search.className="exampleMode smalltext " + document.getElementById('search_class').value;
		document.getElementById('search_ticker').value=0;
		search.value="Example:\n\n     sample,free\n     toasters\n     Seattle\n     -cheese, artichokes\n\nThe above filter would search for feed entries that contain both \'sample\' and \'free\'.  It would also return any entry containing the words \'toasters\' or \'Seattle\'.\nAdding a minus sign to the beginning of any filter line inverts that match, so entries with both the words cheese and artichokes are omitted.";
	} else {
		document.getElementById('search_ticker').value = 1;
	}
	return true;
}

</script>

<input id="url_ticker" value="0" type=hidden>
<input id="url_class" value="" type=hidden>
<input id="search_ticker" value="0" type=hidden>
<input id="search_class" value="0" type=hidden>

<!-- form method="GET" action="/create.php" name="create" onsubmit="cleardefaults();" -->
<form method="GET" name="create" onsubmit="cleardefaults();">

<div class="inbox">
	<div class="label">Feed URL:</div>
	<div class='input'>
   <input name='feed' type='text' class='' value='' onfocus="textfocus('url');" onblur="textblur('url');" id="url">

</div>
</div>

<div class="inbox">
	<div class="label">
		<p>Search text:</p>
		<span class="smalltext">
			<p>One search per line.</p>
			<p>Multiple keywords per search are comma separated.</p>
		   <img src="data:image/gif;base64," class="center" style="margin-top:60px">
		</span>
	</div>
	<div class='input'>
   <textarea name='filters' class='' onfocus="textfocus('search');" onblur="textblur('search');" id="search"></textarea>

</div>

</div>

<div class="inbox">
	<div class="label">&nbsp;</div>
<input class="submit" type="submit" name="go" value="Filter my feed">
</div>
</form>

<script>
init_search();
init_url();
</script>
